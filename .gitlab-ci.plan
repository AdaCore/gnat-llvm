def build():
    # Do not rebuild gcc/gnat
    anod_install("gcc")
    anod_install("gnat-doc", qualifier="ugx")
    anod_build("gnat-llvm")    

def test_fixedbugs():
    anod_install("gnat-llvm")
    anod_test("fixedbugs", qualifier="compiler=gnat-llvm")

def test_acats():
    anod_install("gnat-llvm")
    anod_test("acats", qualifier="compiler=gnat-llvm")
